{% extends 'layout/base.html.twig' %}
    {% block title %}
	    404 : Non trouvé
    {% endblock %}
{% block body %}
	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<div class="error-template">
					<h1>
						Oops!</h1>
					<h2>
						404 Non Trouvé</h2>
					<div class="error-details">
						Désolé, la page demandé n'est pas trouvé !?
					</div>
					<div class="error-actions">
						<a href="{{ path('app_home') }}" class="btn btn-primary btn-lg"><span class="glyphicon glyphicon-home"></span>
							Retour Accueil</a>
					</div>
				</div>
			</div>
		</div>
	</div>
{% endblock %}